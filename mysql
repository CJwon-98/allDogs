CREATE TABLE `ATTACHMENT` (
	`FID`	NUMBER	NOT NULL	COMMENT '파일 번호',
	`PRODUCT_ID`	NUMBER	NOT NULL	COMMENT '상품번호',
	`ORIGIN_NAME`	VARCHAR2(255 BYTE)	NOT NULL	COMMENT '사용자 첨부파일',
	`CHANGE_NAME`	VARCHAR2(255 BYTE)	NOT NULL	COMMENT 'Rename된 파일명',
	`FILE_PATH`	VARCHAR2(1000 BYTE)	NOT NULL	COMMENT '경로',
	`FILE_LEVEL`	NUMBER	NULL	COMMENT '파일순서설정',
	`UPLOAD_DATE`	DATE	NOT NULL	DEFAULT SYSDATE	COMMENT '올린날짜',
	`ASTATUS`	VARCHAR2(1 BYTE)	NOT NULL	DEFAULT 'Y'
);

